switch case
#include <stdio.h>
int main()
{
  int a,b;
  scanf("%d %d",&a,&b);
  int ch;
  scanf("%d",&ch);
  switch(ch)
  {
      case 1:printf("%d",a+b);
      break;
      case 2:printf("%d",a-b);
      break;
      case 3:printf("%d",a*b);
      break;
      default:printf("invalid");
      break;
  }
    
    return 0;
}
