electricity
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   int uc;
    float bill;
    scanf("%d",&uc);
    if(uc<=200)
    {
        bill=uc*0.5;
    }
    else if(uc>200 && uc<=400)
    {
        bill=(uc*0.65)+100;
    }
    else  if(uc>400 && uc<=600)
        
    {
        bill=(uc*0.80)+200;
    }
    else 
    {
        bill=(uc*1.25)+425;
    }
    printf("Rs.%.0f",bill);
    return 0;
}
