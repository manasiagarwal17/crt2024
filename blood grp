blood grp
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int age,w;
    float h;
    scanf("%d %d %f",&age,&w,&h);
    if(age>=18 && w>=45 && h>=5.5)
        printf("He or She can donate blood");
    else
      printf("He or She unable to donate blood");  
    return 0;
}
