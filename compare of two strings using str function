compare of two strings using str function
#include <stdio.h>
#include <string.h>

int main()
{
    
    char str1[50],str2[50];
    scanf("%s",str1);
    scanf("%s",str2);
    int n=strcmp(str1,str2);
    if(n==0)
    printf("strings are equal");
    else
    printf("strings are not equal");

    return 0;
}
