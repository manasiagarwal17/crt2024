cinema day
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int d,m,y,s;
    scanf("%d%d%d%d",&d,&m,&y,&s);
    if((d+m+y)%12==0)
    {
        if(s>50 && s<100)
        {
            printf("Cinema Day");
        }
        else
        {
            printf("Not a Cinema Day");
        }
    }
    else
    {
        printf("Not a Cinema Day");
    }

    return 0;
}
